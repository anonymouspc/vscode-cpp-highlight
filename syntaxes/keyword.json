{
    "scopeName": "keyword.injection",
    "injectionSelector": "L:source.cpp & -comment & -string",
    "patterns": [
        {
            "match": "\\boperator\\b",
            "name": "keyword.other.operator.cpp"
        },
        {
            "match": "\\btemplate\\b",
            "name": "keyword.other.template.cpp"
        },
        {
            "match": "\\btypename\\b",
            "name": "keyword.other.typename.cpp"
        }
    ]
}