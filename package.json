{
	"name": "vscode-cpp-highlight",
	"displayName": "vscode-cpp-highlight",
	"description": "vscode-cpp-highlight",
	"version": "1.0.0",
	"publisher": "anonymouspc",
	"engines": {
		"vscode": "^1.60.0"
	},
	"contributes": {
		"grammars": [
            {
                "path": "./syntaxes/keyword.json",
                "scopeName": "keyword.injection",
                "injectTo": ["source.cpp"]
            },
            {
                "path": "./syntaxes/module.json",
                "scopeName": "module.injection",
                "injectTo": ["source.cpp"]
            },
            {
                "path": "./syntaxes/preprocessor.json",
                "scopeName": "preprocessor.injection",
                "injectTo": ["source.cpp"]
            },
            {
                "path": "./syntaxes/punctuation.json",
                "scopeName": "punctuation.injection",
                "injectTo": ["source.cpp"]
            }
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AnonymoucPC/vscode-cpp-highlight"
	}
}